{
 "scripts": {
  "build": "tsc",
  "makepretty": "prettier --write -l '**/*.ts'",
  "lint": "yarn tslint -p .",
  "precommit": ".hooks/pre-commit",
  "postinstall": "if [ -d .git ]; then git config core.hooksPath .hooks; fi"
 },
 "lint-staged": {
  "*.{js,ts,md}": [
   "yarn prettier --write"
  ],
  "src/**/*.ts": [
   "yarn lint --fix"
  ]
 },
 "devDependencies": {
  "lerna": "^3.20.2",
  "lint-staged": "^10.0.8",
  "prettier": "^2.2.1"
 },
 "license": "MIT",
 "dependencies": {
  "@join-com/node-trace": "^0.1.8",
  "@types/jest": "^26.0.20",
  "@types/node": "^14.14.30",
  "grpc": "^1.24.5",
  "jest": "^26.6.3",
  "jest-cli": "^26.6.3",
  "jest-watch-typeahead": "^0.6.1",
  "protobufjs": "^6.10.2",
  "ts-jest": "^26.5.1",
  "typescript": "^4.2.3"
 }
}
